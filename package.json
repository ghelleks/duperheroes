{
  "name": "duperheroes",
  "version": "1.0.0",
  "description": "A static superhero identification game featuring satirical animal-themed superheroes",
  "main": "public/index.html",
  "scripts": {
    "fetch-heroes": "node scripts/fetch-heroes.js",
    "audit-images": "node scripts/audit-images.js",
    "audit-images:json": "node scripts/audit-images.js --json=audit-report.json",
    "audit-images:strict": "node scripts/audit-images.js --strict",
    "serve": "npx http-server public -p 8080 -o",
    "serve:python": "cd public && python -m http.server 8080"
  },
  "keywords": [
    "game",
    "superhero",
    "web-application",
    "static-site",
    "github-pages"
  ],
  "author": "Gunnar Hellekson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghelleks/duperheroes.git"
  },
  "bugs": {
    "url": "https://github.com/ghelleks/duperheroes/issues"
  },
  "homepage": "https://ghelleks.github.io/duperheroes",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}